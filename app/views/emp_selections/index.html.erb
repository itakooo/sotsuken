<h1>Listing emp_selections</h1>

<table>
  <tr>
    <th>Emp</th>
    <th>Glouping</th>
    <th>Start</th>
    <th>End</th>
    <th>Location</th>
    <th>Result date</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @emp_selections.each do |emp_selection| %>
  <tr>
    <td><%= emp_selection.emp.id %></td>
    <td><%= emp_selection.glouping %></td>
    <td><%= emp_selection.start %></td>
    <td><%= emp_selection.end %></td>
    <td><%= emp_selection.location %></td>
    <td><%= emp_selection.result_date %></td>
    <td><%= emp_selection.result %></td>
    <td><%= link_to 'Show', emp_selection %></td>
    <td><%= link_to 'Edit', edit_emp_selection_path(emp_selection) %></td>
    <td><%= link_to 'Destroy', emp_selection, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

